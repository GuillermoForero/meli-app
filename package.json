{
  "name": "mercado-libre",
  "version": "0.1.0",
  "author": "guillermo-forerom",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom-fourteen --watchAll=false \"--coverageReporters=text-summary\" \"--coverage\"",
    "test:coverage-complete": "react-scripts test --env=jsdom-fourteen --watchAll=false --coverage --testResultsProcessor=jest-sonar-reporter",
    "eject": "react-scripts eject",
    "eslint": "CI=true NODE_ENV=test eslint -c .eslintrc.json -f json -o coverage/eslint-report.json src/; exit 0",
    "lint:fix": "eslint './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@types/jest": "^26.0.15",
    "@types/node": "14.11.5",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "^5.1.3",
    "axios": "^0.21",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^3.4.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "typescript": "^4.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.2",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.51",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "antd": "^4.15.0",
    "axios-mock-adapter": "^1.18.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest-canvas-mock": "^2.3.0",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^7.0.3",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1"
  }
}
